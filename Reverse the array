import java.util.Arrays;
class Main {
    public static void main(String[] args) {
        int[] arr={3,76,98,8,45,65};
        int left=0;
        int right=arr.length-1;
        while(left<right){
            int temp=arr[left];
            arr[left]=arr[right];
            arr[right]=temp;
            left++;
            right--;
        }
        System.out.println("Reversed Array:  "+Arrays.toString(arr));
        System.out.println(arr[0]);
    }
}
